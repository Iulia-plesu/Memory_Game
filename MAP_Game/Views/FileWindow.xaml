        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="File Window" Height="300" Width="400">
    <Grid>
        <Button Content="Category" HorizontalAlignment="Left" VerticalAlignment="Top" Width="100" Height="40" Margin="20" Click="CategoryButton_Click"/>
        <Button Content="New Game" HorizontalAlignment="Left" VerticalAlignment="Top" Width="100" Height="40" Margin="20,70,0,0" Click="NewGameButton_Click"/>
    </Grid>
</Window>
